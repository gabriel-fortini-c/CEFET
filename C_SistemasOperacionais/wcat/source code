#include <stdio.h>
#include <stdlib.h>

int main (int argc, char* argv[])
{
    if (argc < 2) {
        return 0;
    }

    for (int i = 1 ; i < argc ; i++) {
        FILE* arquivo = fopen (argv[i], "r");

        if (arquivo == NULL) {
        printf("wcat: cannot open file\n");
        exit(1);
        }
        
        char linha[1024];

        while (fgets (linha, sizeof(linha), arquivo)) {
            printf ("%s", linha);
        }

        fclose (arquivo);
    }
    
    return 0;
}