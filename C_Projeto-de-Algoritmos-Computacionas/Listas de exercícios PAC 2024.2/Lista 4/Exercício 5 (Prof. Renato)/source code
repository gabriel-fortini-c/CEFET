#include <stdio.h>

float funcionarios (int quantidade)
{
    float maior_salario = 0, salario;
    int matricula;
    
    for (int i=0; i<quantidade; i++)
    {
        printf ("")
        scanf("%d %f", &matricula, &salario);
        if (i == 0 || salario > maior_salario)
        {
            maior_salario = salario;
        }
    }
    return maior_salario;
}

